@model IList<ProductOverviewModel>

@using Nop.Web.Models.Catalog;

@if (Model.Count > 0)
{
    <div class="owl-carousel home-page-product-grid">
        @foreach (var item in Model)
        {
            <div>
                @Html.Partial("_HomeProductBox", item)
            </div>
        }
    </div>
}

