@model SocialModel
@if (!string.IsNullOrEmpty(Model.FacebookLink) || !string.IsNullOrEmpty(Model.TwitterLink) || Model.NewsEnabled || !string.IsNullOrEmpty(Model.YoutubeLink))
{
    <ul class="social-icons">
        @if (!string.IsNullOrEmpty(Model.FacebookLink))
        {
            <li class="facebook" title="Facebook">
                <a href="@Model.FacebookLink" target="_blank">
                    <div class="social-share">
                        <img src="~/themes/BlueMoon/content/images/Facebook.png" />

                    </div>
                </a>
            </li>
        }
        @if (!string.IsNullOrEmpty(Model.TwitterLink))
        {
            <li class="twitter" title="Twitter">
                <a href="@Model.TwitterLink" target="_blank">
                    <div class="social-share">
                        <img src="~/themes/BlueMoon/content/images/twitter.png">
                    </div>
                </a>
            </li>
        }
        @if (Model.NewsEnabled)
        {
            <li class="rss" title="rss">
                <a href="@Url.RouteUrl("NewsRSS", new {languageId = Model.WorkingLanguageId})">
                    <div class="social-share">
                        <img src="~/themes/BlueMoon/content/images/rss.png">
                    </div>
                </a>
            </li>
        }
        @if (!string.IsNullOrEmpty(Model.YoutubeLink))
        {
            <li class="youtube" title="Youtube">
                <a href="@Model.YoutubeLink" target="_blank" rel="noopener noreferrer">
                    <div class="social-share">
                        @* <img src="~/themes/BlueMoon/content/images/youtubenew.png"> *@
                        <svg xmlns="http://www.w3.org/2000/svg" width="38" height="38" viewBox="0 0 38 38" fill="none">
