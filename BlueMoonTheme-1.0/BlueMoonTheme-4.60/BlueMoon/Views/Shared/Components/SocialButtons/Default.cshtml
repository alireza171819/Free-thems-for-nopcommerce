                        </g>
                        <defs>
                        <clipPath id="clip0_445_39">
                        <rect width="38" height="38" fill="white"/>
                        </clipPath>
                        </defs>
                        </svg>
                    </div>
                </a>
            </li>
        }

        @if (!string.IsNullOrEmpty(Model.InstagramLink))
	    {
		<li class="instagram" title="Instagram">
            <a href="@Model.InstagramLink" target="_blank" rel="noopener noreferrer">
                @* @T("Footer.FollowUs.Instagram") *@
                <div class="social-share">
                        @* <img src="~/themes/BlueMoon/content/images/instagramnew.png"> *@
